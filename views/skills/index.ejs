<!DOCTYPE html>
<html>
  <head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel='stylesheet' href='/stylesheets/style.css' />
    <title>My Dev Skills List</title>
  </head>
  <body>
    <h1>My Dev Skills</h1>
    <ul>
        <% skills.forEach(function(skill) { %>
          <li>
              <a href="/skills/<%= skill.id %>"><%= skill.skill %></a>
              <%= skill.skills %> 
               - 
              <%= skill.learned ? 'learned' : 'not learned' %>
          </li>  
        <% }); %>
    </ul>
    <a href="/skills/new">Add New Skill</a>
</body>
</html>